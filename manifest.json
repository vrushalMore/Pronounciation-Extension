{
    "manifest_version": 3,
    "name": "Pronounciation",
    "version": "1.0",
    "description": "Chrome extension that helps you understand pronounciation",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab"],
    "icons": {
      "128": "icon.png"
    }
}